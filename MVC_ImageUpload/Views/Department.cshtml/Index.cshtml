@model IEnumerable<MVC_ImageUpload.Models.Department>

<h2>Departments</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var department in Model)
        {
            <tr>
                <td>@department.Name</td>
            </tr>
        }
    </tbody>
</table>